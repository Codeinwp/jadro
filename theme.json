{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "no-title-space",
			"postTypes": ["page"],
			"title": "No Title and Space"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true,
			"palette": [
				{
					"color": "#062d48",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#9164ff",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f3f6fb",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#2f2f2f",
					"name": "Darkish",
					"slug": "darkish"
				}
			]
		},
		"layout": {
			"contentSize": "760px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter",
					"slug": "inter",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/inter_normal_300.ttf"]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/inter_normal_400.ttf"]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": ["file:./assets/fonts/inter_normal_500.ttf"]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": ["file:./assets/fonts/inter_normal_600.ttf"]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/inter_normal_700.ttf"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "X Small",
					"size": "0.69rem",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "0.89rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.19rem",
						"min": "1rem"
					},
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"fluid": {
						"max": "1.58rem",
						"min": "1.2rem"
					},
					"name": "Medium",
					"size": "1.58rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2.11rem",
						"min": "1.44rem"
					},
					"name": "Large",
					"size": "2.11rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.81rem",
						"min": "1.73rem"
					},
					"name": "X Large",
					"size": "2.81rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "2.07rem"
					},
					"name": "2X Large",
					"size": "3.75rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "5rem",
						"min": "2.49rem"
					},
					"name": "3X Large",
					"size": "5rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"max": "6.66rem",
						"min": "2.99rem"
					},
					"name": "4X Large",
					"size": "6.66rem",
					"slug": "xxxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"typography": {
					"lineHeight": "1"
				},
				"variations": {
					"outline": {
						"color": {
							"text": "#9164ff"
						}
					}
				}
			},
			"core/site-title": {
			  "typography": {
			    "fontSize": "var:preset|font-size|x-large"
			  }
			}
		},
		"color": {
			"background": "var:preset|color|white",
			"text": "#062d48"
		},
		"elements": {
			"button": {
				"color": {
					"background": "#9164ff"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.5"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "1.6"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "1.6"
				}
			},
			"heading": {
				"color": {
					"text": "#062d48"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|inter",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"link": {
				"color": {
					"text": "#9164ff"
				}
			}
		},
		"spacing": {
			"blockGap": "24px",
			"padding": {
				"bottom": "0px",
				"left": "20px",
				"right": "20px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|inter",
			"fontSize": "var:preset|font-size|base",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}
